<Window x:Class="WPFUI.Views.AddPassengerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFUI.Views"
        mc:Ignorable="d"
        Title="AddPassengerWindow" Height="450" Width="800">
    <StackPanel Margin="10">
        <TextBlock Text="Имя:"/>
        <TextBox x:Name="FirstNameTextBox" Margin="0,5"/>
        <TextBlock Text="Фамилия:"/>
        <TextBox x:Name="SecondNameTextBox" Margin="0,5"/>
        <Button Content="Добавить" Click="AddButton_Click" Margin="0,10"/>
    </StackPanel>
</Window>
